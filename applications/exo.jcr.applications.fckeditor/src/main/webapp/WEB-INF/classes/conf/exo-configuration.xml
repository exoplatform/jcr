      <value-param>
        <name>def-folder-node-type</name>
        <value>nt:folder</value>
      </value-param>
      
      <value-param>
        <name>def-file-node-type</name>
        <value>nt:file</value>
      </value-param>
      
      <value-param>
        <name>def-file-mimetype</name>
        <value>text/plain</value>
      </value-param>
      
      <value-param>
